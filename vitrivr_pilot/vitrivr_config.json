{
  "api": {
    "host" : "$EXT_HOST",
    "port" : 4567,
    "protocol_http": "http",
    "protocol_ws": "ws",
    "ping_interval": 10000
  },
  "resources": {
    "host_thumbnails" : "http://$EXT_HOST:9021/thumbnails/:o/:s:x",
    "host_objects": "http://$EXT_HOST:9021/objects/:p",
    "suffix_default":"jpg",
    "suffix": {
      "IMAGE": "png",
      "VIDEO": "png"
    }
  },
  "mlt" : ["globalcolor", "localcolor", "edge"],
  "query": {
    "history": 5,
    "options": {
      "image": true,
      "audio": false,
      "model3d": false,
      "motion": false,
      "text": true,
      "tag": false,
      "semantic": true,
      "boolean": true,
      "pose": true
    },
    "config": {
      "hints": ["exact"],
      "neighboringSegmentLookupCount": 5,
      "neighboringSegmentLookupAllCount": 2000
    },
    "text": {
      "categories": [
        ["ocr", "Text on Screen"],
        ["tagsft", "Tags"]
      ]
    },
    "boolean": [
      ["Annotation", "TEXT", "features_wsdmtiannotations.annotation"],
      ["Object", "TEXT", "features_wsdmtiannotations.object"]
    ]
  }
}
